{
  "expo": {
    "backgroundColor": "#0a0f14",
    "userInterfaceStyle": "automatic",
    "name": "wsei-lab-mobile",
    "slug": "wsei-lab-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elkolorado.fusionworldscanner",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a1f2e"
      },
      "package": "com.elkolorado.fusionworldscanner"
    },
    "androidStatusBar": {
      "backgroundColor": "#0a0f14",
      "barStyle": "light-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "backgroundColor": "#0a0f14",
      "barStyle": "light",
      "translucent": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a1f2e"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#0a0f14",
          "barStyle": "light",
          "position": "relative"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1dd5bc0f-dfc7-4997-83b6-78f5d8503dc6"
      }
    }
  }
}